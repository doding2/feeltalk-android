<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/feeltalk_nav_graph"
    app:startDestination="@id/signUpFragment">

    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.clonect.feeltalk.new_presentation.ui.signUp.SignUpFragment"
        android:label="fragment_sign_up"
        tools:layout="@layout/fragment_sign_up" >
        <action
            android:id="@+id/action_signUpFragment_to_signUpNavigationFragment"
            app:destination="@id/signUpNavigationFragment" />
        <action
            android:id="@+id/action_signUpFragment_to_mainNavigationFragment"
            app:destination="@id/mainNavigationFragment"
            app:popUpTo="@id/signUpFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/signUpNavigationFragment"
        android:name="com.clonect.feeltalk.new_presentation.ui.signUpNavigation.SignUpNavigationFragment"
        android:label="fragment_sign_up_navigation"
        tools:layout="@layout/fragment_sign_up_navigation" >
        <action
            android:id="@+id/action_signUpFragment_pop_back"
            app:destination="@id/signUpFragment"
            app:popUpTo="@id/signUpFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_signUpNavigationFragment_to_mainNavigationFragment"
            app:destination="@id/mainNavigationFragment"
            app:popUpTo="@id/signUpFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_signUpNavigationFragment_to_agreementSensitiveDetailFragment"
            app:destination="@id/agreementSensitiveDetailFragment" />
        <action
            android:id="@+id/action_signUpNavigationFragment_to_agreementPrivacyDetailFragment"
            app:destination="@id/agreementPrivacyDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/mainNavigationFragment"
        android:name="com.clonect.feeltalk.new_presentation.ui.mainNavigation.MainNavigationFragment"
        android:label="fragment_main_navigation"
        tools:layout="@layout/fragment_main_navigation" >
        <action
            android:id="@+id/action_mainNavigationFragment_to_addChallengeFragment"
            app:destination="@id/addChallengeFragment" />
        <action
            android:id="@+id/action_mainNavigationFragment_to_contentsShareFragment"
            app:destination="@id/contentsShareFragment" />
        <action
            android:id="@+id/action_mainNavigationFragment_to_ongoingChallengeDetailFragment"
            app:destination="@id/ongoingChallengeDetailFragment" />
        <action
            android:id="@+id/action_mainNavigationFragment_to_completedChallengeDetailFragment"
            app:destination="@id/completedChallengeDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/agreementPrivacyDetailFragment"
        android:name="com.clonect.feeltalk.new_presentation.ui.agreementPrivacyDetail.AgreementPrivacyDetailFragment"
        android:label="fragment_agreement_privacy_detail"
        tools:layout="@layout/fragment_agreement_privacy_detail" />
    <fragment
        android:id="@+id/agreementSensitiveDetailFragment"
        android:name="com.clonect.feeltalk.new_presentation.ui.agreementSensitiveDetail.AgreementSensitiveDetailFragment"
        android:label="fragment_agreement_sensitive_detail"
        tools:layout="@layout/fragment_agreement_sensitive_detail" />
    <fragment
        android:id="@+id/addChallengeFragment"
        android:name="com.clonect.feeltalk.new_presentation.ui.mainNavigation.challenge.addChallenge.AddChallengeFragment"
        android:label="fragment_add_challenge"
        tools:layout="@layout/fragment_add_challenge" />
    <fragment
        android:id="@+id/contentsShareFragment"
        android:name="com.clonect.feeltalk.new_presentation.ui.mainNavigation.chatNavigation.contentsShare.ContentsShareFragment"
        android:label="ContentsShareFragment"
        tools:layout="@layout/fragment_contents_share"/>
    <fragment
        android:id="@+id/ongoingChallengeDetailFragment"
        android:name="com.clonect.feeltalk.new_presentation.ui.mainNavigation.challenge.ongoingDetail.OngoingDetailFragment"
        android:label="OngoingChallengeDetailFragment"
        tools:layout="@layout/fragment_ongoing_detail" />
    <fragment
        android:id="@+id/completedChallengeDetailFragment"
        android:name="com.clonect.feeltalk.new_presentation.ui.mainNavigation.challenge.completedDetail.CompletedDetailFragment"
        android:label="CompletedChallengeDetailFragment"
        tools:layout="@layout/fragment_completed_detail"/>
    <fragment
        android:id="@+id/serverDownFragment"
        android:name="com.clonect.feeltalk.new_presentation.ui.serverDown.ServerDownFragment"
        android:label="fragment_server_down"
        tools:layout="@layout/fragment_server_down" />
    <fragment
        android:id="@+id/networkErrorFragment"
        android:name="com.clonect.feeltalk.new_presentation.ui.networkError.NetworkErrorFragment"
        android:label="fragment_network_error"
        tools:layout="@layout/fragment_network_error" />
</navigation>