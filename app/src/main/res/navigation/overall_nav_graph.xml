<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/total_nav_graph"
    app:startDestination="@id/signUpFragment">

    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.clonect.feeltalk.new_presentation.ui.signUp.SignUpFragment"
        android:label="fragment_sign_up"
        tools:layout="@layout/fragment_sign_up" >
        <action
            android:id="@+id/action_signUpFragment_to_bottomNavigationFragment"
            app:destination="@id/bottomNavigationFragment"
            app:enterAnim="@anim/slide_from_bottom"
            app:exitAnim="@anim/do_noting"
            app:popEnterAnim="@anim/do_noting"
            app:popExitAnim="@anim/slide_to_bottom"
            app:popUpTo="@id/signUpFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_signUpFragment_to_guideFragment"
            app:destination="@id/guideFragment"
            app:enterAnim="@anim/slide_from_bottom"
            app:exitAnim="@anim/do_noting"
            app:popEnterAnim="@anim/do_noting"
            app:popExitAnim="@anim/slide_to_bottom" />
        <action
            android:id="@+id/action_signUpFragment_to_userGenderInputFragment"
            app:destination="@id/userGenderInputFragment"
            app:enterAnim="@anim/slide_from_bottom"
            app:exitAnim="@anim/do_noting"
            app:popEnterAnim="@anim/do_noting"
            app:popExitAnim="@anim/slide_to_bottom"
            app:popUpTo="@id/signUpFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/bottomNavigationFragment"
        android:name="com.clonect.feeltalk.presentation.ui.bottom_navigation.BottomNavigationFragment"
        android:label="fragment_bottom_navigation"
        tools:layout="@layout/fragment_bottom_navigation" >
        <action
            android:id="@+id/action_bottomNavigationFragment_to_todayQuestionFragment"
            app:destination="@id/todayQuestionFragment"
            app:enterAnim="@anim/slide_from_right"
            app:exitAnim="@anim/do_noting"
            app:popEnterAnim="@anim/do_noting"
            app:popExitAnim="@anim/slide_to_right" />
        <action
            android:id="@+id/action_bottomNavigationFragment_to_newsFragment"
            app:destination="@id/newsFragment"
            app:enterAnim="@anim/slide_from_bottom"
            app:exitAnim="@anim/do_noting"
            app:popEnterAnim="@anim/do_noting"
            app:popExitAnim="@anim/slide_to_bottom" />
        <action
            android:id="@+id/action_bottomNavigationFragment_to_coupleSettingFragment"
            app:destination="@id/coupleSettingFragment"
            app:enterAnim="@anim/slide_from_bottom"
            app:exitAnim="@anim/do_noting"
            app:popEnterAnim="@anim/do_noting"
            app:popExitAnim="@anim/slide_to_bottom" />
        <action
            android:id="@+id/action_bottomNavigationFragment_to_signUpFragment"
            app:destination="@id/signUpFragment"
            app:enterAnim="@anim/do_noting"
            app:exitAnim="@anim/slide_to_bottom"
            app:popEnterAnim="@anim/slide_from_bottom"
            app:popExitAnim="@anim/do_noting"
            app:popUpTo="@id/bottomNavigationFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_bottomNavigationFragment_to_keyRestoringRequestFragment"
            app:destination="@id/keyRestoringRequestFragment"
            app:enterAnim="@anim/slide_from_bottom"
            app:exitAnim="@anim/do_noting"
            app:popEnterAnim="@anim/do_noting"
            app:popExitAnim="@anim/slide_to_bottom" />
        <action
            android:id="@+id/action_bottomNavigationFragment_to_keyRestoringAcceptFragment"
            app:destination="@id/keyRestoringAcceptFragment"
            app:enterAnim="@anim/slide_from_bottom"
            app:exitAnim="@anim/do_noting"
            app:popEnterAnim="@anim/do_noting"
            app:popExitAnim="@anim/slide_to_bottom" />
        <action
            android:id="@+id/action_bottomNavigationFragment_to_privacyPolicyFragment"
            app:destination="@id/privacyPolicyFragment"
            app:enterAnim="@anim/slide_from_bottom"
            app:exitAnim="@anim/do_noting"
            app:popEnterAnim="@anim/do_noting"
            app:popExitAnim="@anim/slide_to_bottom" />
    </fragment>
    <fragment
        android:id="@+id/todayQuestionFragment"
        android:name="com.clonect.feeltalk.presentation.ui.today_question.TodayQuestionFragment"
        android:label="fragment_today_question"
        tools:layout="@layout/fragment_today_question" >
    </fragment>
    <fragment
        android:id="@+id/newsFragment"
        android:name="com.clonect.feeltalk.presentation.ui.news.NewsFragment"
        android:label="fragment_news"
        tools:layout="@layout/fragment_news" />
    <fragment
        android:id="@+id/coupleSettingFragment"
        android:name="com.clonect.feeltalk.presentation.ui.couple_setting.CoupleSettingFragment"
        android:label="fragment_couple_setting"
        tools:layout="@layout/fragment_couple_setting" >
        <action
            android:id="@+id/action_coupleSettingFragment_to_coupleRegistrationFragment"
            app:destination="@id/coupleRegistrationFragment"
            app:enterAnim="@anim/do_noting"
            app:exitAnim="@anim/slide_to_bottom"
            app:popEnterAnim="@anim/slide_from_bottom"
            app:popExitAnim="@anim/do_noting"
            app:popUpTo="@id/coupleSettingFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/coupleRegistrationFragment"
        android:name="com.clonect.feeltalk.presentation.ui.couple_registration.CoupleRegistrationFragment"
        android:label="fragment_couple_registration"
        tools:layout="@layout/fragment_couple_registration" >
        <action
            android:id="@+id/action_coupleRegistrationFragment_to_coupleRegistrationDoneFragment"
            app:destination="@id/coupleRegistrationDoneFragment"
            app:enterAnim="@anim/slide_from_right"
            app:exitAnim="@anim/do_noting"
            app:popEnterAnim="@anim/do_noting"
            app:popExitAnim="@anim/slide_to_right" />
    </fragment>
    <fragment
        android:id="@+id/userNicknameInputFragment"
        android:name="com.clonect.feeltalk.presentation.ui.input.UserNicknameInputFragment"
        android:label="fragment_user_nickname_input"
        tools:layout="@layout/fragment_user_nickname_input" >
        <action
            android:id="@+id/action_userNicknameInputFragment_to_userBirthInputFragment"
            app:destination="@id/userBirthInputFragment"
            app:enterAnim="@anim/slide_from_right"
            app:exitAnim="@anim/do_noting"
            app:popEnterAnim="@anim/do_noting"
            app:popExitAnim="@anim/slide_to_right" />
    </fragment>
    <fragment
        android:id="@+id/userBirthInputFragment"
        android:name="com.clonect.feeltalk.presentation.ui.input.UserBirthInputFragment"
        android:label="fragment_user_birth_input"
        tools:layout="@layout/fragment_user_birth_input" >
        <action
            android:id="@+id/action_userBirthInputFragment_to_coupleAnniversaryInputFragment"
            app:destination="@id/coupleAnniversaryInputFragment"
            app:enterAnim="@anim/slide_from_right"
            app:exitAnim="@anim/do_noting"
            app:popEnterAnim="@anim/do_noting"
            app:popExitAnim="@anim/slide_to_right" />
    </fragment>
    <fragment
        android:id="@+id/coupleAnniversaryInputFragment"
        android:name="com.clonect.feeltalk.presentation.ui.input.CoupleAnniversaryInputFragment"
        android:label="fragment_couple_anniversary_input"
        tools:layout="@layout/fragment_couple_anniversary_input" >
        <action
            android:id="@+id/action_coupleAnniversaryInputFragment_to_guideFragment"
            app:destination="@id/guideFragment"
            app:enterAnim="@anim/slide_from_bottom"
            app:exitAnim="@anim/do_noting"
            app:popEnterAnim="@anim/do_noting"
            app:popExitAnim="@anim/slide_to_bottom" />
    </fragment>
    <fragment
        android:id="@+id/coupleRegistrationDoneFragment"
        android:name="com.clonect.feeltalk.presentation.ui.couple_registration_done.CoupleRegistrationDoneFragment"
        android:label="fragment_couple_registration_done"
        tools:layout="@layout/fragment_couple_registration_done" >
        <action
            android:id="@+id/action_coupleRegistrationDoneFragment_to_bottomNavigationFragment"
            app:destination="@id/bottomNavigationFragment"
            app:popUpTo="@id/coupleRegistrationDoneFragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/slide_from_bottom"
            app:exitAnim="@anim/do_noting"
            app:popEnterAnim="@anim/do_noting"
            app:popExitAnim="@anim/slide_to_bottom"/>
    </fragment>
    <fragment
        android:id="@+id/guideFragment"
        android:name="com.clonect.feeltalk.presentation.ui.guide.GuideFragment"
        android:label="fragment_guide"
        tools:layout="@layout/fragment_guide" >
        <action
            android:id="@+id/action_guideFragment_to_coupleRegistrationFragment"
            app:destination="@id/coupleRegistrationFragment"
            app:enterAnim="@anim/slide_from_bottom"
            app:exitAnim="@anim/do_noting"
            app:popEnterAnim="@anim/do_noting"
            app:popExitAnim="@anim/slide_to_bottom" />
    </fragment>
    <fragment
        android:id="@+id/keyRestoringAcceptFragment"
        android:name="com.clonect.feeltalk.presentation.ui.key_restoring_accept.KeyRestoringAcceptFragment"
        android:label="fragment_key_restoring_accept"
        tools:layout="@layout/fragment_key_restoring_accept" />
    <fragment
        android:id="@+id/keyRestoringRequestFragment"
        android:name="com.clonect.feeltalk.presentation.ui.key_restoring_request.KeyRestoringRequestFragment"
        android:label="fragment_key_restoring_request"
        tools:layout="@layout/fragment_key_restoring_request" />
    <fragment
        android:id="@+id/privacyPolicyFragment"
        android:name="com.clonect.feeltalk.presentation.ui.privacy_policy.PrivacyPolicyFragment"
        android:label="fragment_privacy_policy"
        tools:layout="@layout/fragment_privacy_policy" />
    <fragment
        android:id="@+id/userGenderInputFragment"
        android:name="com.clonect.feeltalk.presentation.ui.input.UserGenderInputFragment"
        android:label="fragment_user_gender_input"
        tools:layout="@layout/fragment_user_gender_input" >
        <action
            android:id="@+id/action_userGenderInputFragment_to_userNicknameInputFragment"
            app:destination="@id/userNicknameInputFragment"
            app:enterAnim="@anim/slide_from_right"
            app:exitAnim="@anim/do_noting"
            app:popEnterAnim="@anim/do_noting"
            app:popExitAnim="@anim/slide_to_right" />
    </fragment>
</navigation>